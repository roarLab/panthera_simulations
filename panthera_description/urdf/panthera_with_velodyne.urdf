  <gazebo reference="rr_wheel_link">
    <mu1 value="0.01" />
    <mu2 value="0.01" />
    <kp  value="1000000.0" />
    <kd  value="1.0" />
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- vlp-16 with lower sample size -->
  <xacro:include filename="$(find velodyne_description)/urdf/VLP-16.urdf.xacro"/>
  <xacro:VLP-16 parent="base_link" name="velodyne" topic="/velodyne_points" organize_cloud="false" hz="10" samples="440" gpu="false">
    <origin xyz="0 0 1.03" rpy="0 0 0" />
  </xacro:VLP-16>

  <!-- controllers -->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/</robotNamespace>
    </plugin>
  </gazebo>

  <transmission name="base_left_joint_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="base_left_joint_motor">
      <hardwareInterface>PositionJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="base_left_joint">
      <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
    </joint>
  </transmission>

